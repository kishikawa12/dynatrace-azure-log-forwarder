{
  "scriptFile": "main.py",
  "bindings": [
    {
      "type": "blobTrigger",
      "name": "myblob",
      "direction": "in",
      "path": "%BLOB_STORAGE_PATH%",
      "connection": "STORAGE_ACCOUNT_CONNECTION"
    }
  ],
  "retry": {
    "strategy": "exponentialBackoff",
    "maxRetryCount": 5,
    "minimumInterval": "00:00:10",
    "maximumInterval": "00:05:00"
  }
}